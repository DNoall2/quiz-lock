{
  "manifest_version": 2,
  "name": "Note Quiz Browser Extension",
  "version": "1.0",
  "description": "A browser extension that allows you to take quizzes on your notes.",
  "permissions": ["storage", "webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Note Quiz",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": ["blocked.html"]
}
